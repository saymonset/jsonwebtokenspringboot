    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow6" sourceRef="businessruletask1" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow7" sourceRef="exclusivegateway1" targetRef="servicetask3">
    
    <conditionExpression xsi:type="tFormalExpression">${3 == 2}</conditionExpression>
    
    </sequenceFlow>
    <serviceTask id="servicetask4" 
     flowable:class="com.service.BillAttachmentService"
    name="Service Task"></serviceTask>
    
    <sequenceFlow id="flow8" sourceRef="exclusivegateway1"  targetRef="servicetask4">
     <conditionExpression xsi:type="tFormalExpression">${2 == 2}</conditionExpression>
    
    
    </sequenceFlow>
    <sequenceFlow id="flow9" sourceRef="servicetask4" targetRef="endevent1"></sequenceFlow>
    
